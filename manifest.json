{
	"manifest_version": 2,
	"name": "TypeLingo FF",
	"version": "1.0",
	"developer": {
		"name": "Katharine Chui",
		"url": "https://github.com/Kethen/TypeLingoFF/"
	},
	"description": "Re-enabling keyboard responses in Duolingo",

	"icons": {
		"128": "icons/128.png"
	},

	"background": {
		"scripts": ["typelingoff.js"]
	},

	"permissions": ["*://*.duolingo.com/*/sessions", "webRequest", "webRequestBlocking"]
}
